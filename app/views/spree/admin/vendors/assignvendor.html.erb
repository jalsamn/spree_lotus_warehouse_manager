<h1>Listing Vendors</h1>

<table>
  <thead>
    <tr>
      <th>Barcode</th>
      <th>Product Name</th>
      <th>InStock</th>

      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @vars.each do |var| %>
      <tr>
        <td><%= var.barcode %></td>
        <td><%= var.name %></td>
        <td><%= var.total_on_hand %></td>

      </tr>
    <% end %>
  </tbody>
</table>
